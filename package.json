{
  "name": "@tawk-agents-sdk/core",
  "version": "1.0.0",
  "description": "Production-ready AI agent framework with multi-agent orchestration, handoffs, guardrails, and observability. Built on Vercel AI SDK.",
  "author": "Tawk.to <support@tawk.to>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/atawk/agents-sdk.git"
  },
  "homepage": "https://github.com/atawk/agents-sdk#readme",
  "bugs": {
    "url": "https://github.com/atawk/agents-sdk/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "ai",
    "agents",
    "openai",
    "anthropic",
    "google",
    "langfuse",
    "mcp",
    "vercel-ai-sdk",
    "multi-agent",
    "agent-framework",
    "typescript",
    "production-ready",
    "tracing",
    "guardrails",
    "human-in-the-loop"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "build:check": "tsc --noEmit",
    "test": "ts-node tests/run-all-tests.ts",
    "test:basic": "ts-node tests/01-basic-agent.test.ts",
    "test:multi": "ts-node tests/02-multi-agent.test.ts",
    "test:stream": "ts-node tests/03-streaming.test.ts",
    "test:guards": "ts-node tests/04-guardrails.test.ts",
    "test:sessions": "ts-node tests/05-sessions.test.ts",
    "test:langfuse": "ts-node tests/06-langfuse-tracing.test.ts",
    "test:output": "ts-node tests/09-structured-output.test.ts",
    "test:storage": "ts-node tests/17-session-storage-e2e.test.ts",
    "test:summarization": "ts-node tests/16-auto-summarization.test.ts",
    "test:multi-model": "ts-node tests/14-multi-model-handoff.test.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "docker:logs": "docker-compose logs -f",
    "test:errors": "ts-node tests/10-error-handling.test.ts",
    "test:watch": "nodemon --watch src --watch tests --ext ts --exec ts-node tests/run-all-tests.ts",
    "example": "ts-node examples/complete-examples.ts",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "@ai-sdk/groq": "^2.0.29",
    "ai": "^4.0.0",
    "langfuse": "^3.30.3",
    "zod": "^3.25.40"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^1.0.0",
    "@ai-sdk/google": "^1.0.0",
    "@ai-sdk/openai": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/openai": {
      "optional": true
    },
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "@modelcontextprotocol/sdk": "^1.17.2",
    "ioredis": "^5.4.2",
    "mongodb": "^6.12.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "dotenv": "^16.4.7",
    "eslint": "^9.17.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
